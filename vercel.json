{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "framework": null,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/vercel.js"
        }
    ],
    "functions": {
        "api/vercel.js": {
            "includeFiles": "dist/**/*"
        }
    }
}
